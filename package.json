{
  "name": "10corp-html",
  "version": "1.0.0",
  "description": "10Corp HTML Template with SASS",
  "main": "index.html",
  "scripts": {
    "sass": "sass sass/main.scss css/style.css",
    "sass:watch": "sass --watch sass/main.scss css/style.css",
    "sass:compressed": "sass sass/main.scss css/style.min.css --style=compressed",
    "sass:expanded": "sass sass/main.scss css/style.css --style=expanded",
    "build": "npm run sass:compressed",
    "dev": "npm run sass:watch"
  },
  "keywords": [
    "html",
    "sass",
    "scss",
    "foundation",
    "hosting",
    "template"
  ],
  "author": "10Corp",
  "license": "ISC",
  "devDependencies": {
    "sass": "^1.69.0"
  }
}
